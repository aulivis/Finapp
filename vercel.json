{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "contexta.hu"
        }
      ],
      "destination": "https://www.contexta.hu/:path*",
      "permanent": true
    }
  ]
}
